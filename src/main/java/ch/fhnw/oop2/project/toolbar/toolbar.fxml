<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>

<?import javafx.scene.layout.HBox?>
<ToolBar prefHeight="60.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
  <items>
    <Button fx:id="save" mnemonicParsing="false" />
      <Separator orientation="VERTICAL" />
      <Button fx:id="add" mnemonicParsing="false" />
      <Button fx:id="remove" mnemonicParsing="false" />
      <Separator orientation="VERTICAL" />
      <Button fx:id="undo" mnemonicParsing="false" />
      <Button fx:id="redo" mnemonicParsing="false" />
      <HBox HBox.hgrow="ALWAYS" />
      <TextField styleClass="filter" />
  </items>
</ToolBar>
